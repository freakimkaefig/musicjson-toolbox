{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "amd": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "strict": 0,
    "eol-last": 2,
    "indent": [2, 2, { "VariableDeclarator": 1 }],
    "camelcase": 2,
    "quotes": [1, "single"],
    "semi": 2,
    "comma-style": 2,
    "curly": 2,
    "brace-style": 2,
    "yoda": 2,
    "eqeqeq": [2, "allow-null"],
    "valid-jsdoc": 2,
    "no-unreachable": 2,
    "no-trailing-spaces": 1,
    "no-shadow": 2
  }
}
