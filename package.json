{
  "name": "musicjson-toolbox",
  "version": "1.2.1",
  "description": "A toolbox for comparing musicjson objects",
  "main": "musicjson-toolbox.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec",
    "lint": "./node_modules/.bin/eslint ./**/*.js",
