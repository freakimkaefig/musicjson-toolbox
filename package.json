  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-stats": "^0.3.0",
    "commander": "^2.9.0",
    "eslint": "^2.9.0",
    "fs": "0.0.2",
    "istanbul": "^0.4.3",
    "jsdoc-to-markdown": "^1.3.6",
    "minifier": "^0.7.1",
    "mocha": "^2.4.5"
  }
